package demo;

public class Recursion
{

	
	public void m1()
	{
		
		
		for (int i = 0; i <10; i++)
		{
		m2();
		}
		System.out.println("iam m1();");
		
	}
	

	
	public void m2()
	{
		
		for (int i = 0; i < 10; i++) 
		{
			
		
		m1();
	}
		System.out.println("iam m12();");
	}
	public static void main(String[] args) {
		Recursion recursion= new Recursion();
		recursion.m2();
	}
	
}
